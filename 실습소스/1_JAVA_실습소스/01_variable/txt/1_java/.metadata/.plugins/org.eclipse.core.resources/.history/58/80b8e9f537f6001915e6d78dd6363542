package com.kh.operator;

public class D_Comparison {
	// 비교 연산자 (== 관계연산자) 
	/* - 두 피연산자를 비교하는 연산자
	 * - 비교 연산자는 조건을 만족하면 true/ 아니면 false를 반환함.
	 * 
	 * a < b	: a가 b보다 작은가? (a값이 b값 미만인가?)
	 * a > b	: a가 b보다 큰가? (a값이 b값을 초과했는가?)
	 * a == b	: a와 b가 서로 같은가?
	 * a != b	: a와 b가 서로 다른가?
	 * a <= b	: a가 b보다 작거나 같은가?(a값이 b값 이하인가?)
	 * a >= b	: a가 b보다 크거나 같은가?(a값이 b값 이상인가?)
	 * 
	 * */
	
	public void method1() {
		int a = 10;
		int b = 25;
		
		boolean result1;
		boolean result2;
		boolean result3;
		
		
		
	}
}
